<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="borderMenu" prefHeight="768.0" prefWidth="1024.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cipriano.view.EditorController">
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                  <MenuItem mnemonicParsing="false" onAction="#fileClose" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Action">
                  <items>
                  <MenuItem mnemonicParsing="false" onAction="#compileAndRun" text="Compile and Run" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#helpAbout" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
   <center>
      <TextArea fx:id="textArea" cache="true" onKeyReleased="#analisaSemantica" prefHeight="200.0" prefWidth="200.0" wrapText="true" BorderPane.alignment="CENTER">
      </TextArea>
   </center>
      </BorderPane>
